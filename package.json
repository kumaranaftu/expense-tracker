{
  "name": "expense-tracker",
  "version": "1.0.0",
  "description": "Web to tracker your daily expenses",
  "scripts": {
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=src/spec/config/jasmine.json",
    "client": "webpack-dev-server --config ./src/client/config/webpack.dev.js --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js",
    "build": "webpack --config ./src/client/config/webpack.prod.js --mode production",
    "start": "npm run build && node src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumaranaftu/expense-tracker.git"
  },
  "author": "Kumaran Dhanasekaran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumaranaftu/expense-tracker/issues"
  },
  "homepage": "https://github.com/kumaranaftu/expense-tracker#readme",
  "dependencies": {
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.5.8",
    "tracer": "^0.9.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "jasmine": "^3.4.0",
    "sinon": "^7.3.2",
    "sinon-express-mock": "^2.2.0",
    "nodemon": "^1.19.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "clean-webpack-plugin": "^2.0.2",
    "babel-loader": "^8.0.6",
    "style-loader": "^0.23.1",
    "css-loader": "^2.1.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "file-loader": "^3.0.1",
    "webpack-merge": "^4.2.1",
    "mini-css-extract-plugin": "^0.6.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack-md5-hash": "0.0.6",
    "concurrently": "^4.1.0"
  }
}
